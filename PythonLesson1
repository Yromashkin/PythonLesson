time=int(input("Введите время в секундах : "))

week=time/604800
print(week)

day=time/86400
print(day)

hours=time/3600
print(hours)

minutes=time/60
print(minutes)

second=time
print(second)

print("Время =")
print(week,"Неделя")
print(day,"День")
print(hours,"Часов")
print(minutes,"Минут")
print(second,"Секунд")
